<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <EditText
        android:id="@+id/edtResult"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:gravity="left"
        android:text="结果" />

    <Button
        android:id="@+id/btn_none"
        android:textSize="13dp"
        android:text=""
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_string_result"
        android:textSize="13dp"
        android:text=""
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_string_result_params"
        android:textSize="13dp"
        android:text=""
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_string_result_params_data"
        android:textSize="12dp"
        android:text=""
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_object_params"
        android:textSize="13dp"
        android:text=""
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_object_params_data"
        android:textSize="13dp"
        android:text=""
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_error"
        android:textSize="13dp"
        android:text="error"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnXUtils"
        android:textSize="13dp"
        android:text="xUtils"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>
